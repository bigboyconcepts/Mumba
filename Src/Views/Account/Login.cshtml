@using SamMiller.Mumba.Models.AccountViewModels
@model LoginRequest
<div class=container>
@{ 
	ViewData["Title"] = "Login";
}


<form method="post">
		<input asp-for="Username" placeholder="Username">
	
		<input asp-for="Password" placeholder="Password">
		<p>
			<label>
				<input  asp-for="RememberMe" type="checkbox" />
				<span>Remember Me?</span>
			</label>
		</p>
		<button class="btn-large center-align" type="submit">
			Login
		</button>
</form>
</div>